[
 {rabbit,
  [
   {loopback_users, []},
    { ssl_listeners, [5671] },
    { ssl_options, [
      {cacertfile,"/home/certificates/myCA.pem"},
      {certfile,"/home/certificates/x800dm.com.pem"},
      {keyfile,"/home/certificates/dm-rabbitmq.pem"},
      {verify,verify_peer},
      {fail_if_no_peer_cert,true},
      {versions, ['tlsv1.2', 'tlsv1.1']}
    ]},
   {default_vhost,       "/"},
   {default_user,        "guest"},
   {default_pass,        "guest"},
   {default_permissions, [".*", ".*", ".*"]},
   {log_levels, [{connection,debug}]}	
  ]}
].